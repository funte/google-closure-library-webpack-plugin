{
  "name": "google-closure-library-webpack-plugin",
  "version": "2.0.3",
  "description": "Webpack plugin for google-closure-library, separate from closure-webpack-plugin",
  "main": "./dist/plugin.js",
  "types": "./dist/plugin.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "高国文(funte@outlook.com)",
  "homepage": "https://github.com/funte/google-closure-library-webpack-plugin",
  "repository": "https://github.com/funte/google-closure-library-webpack-plugin",
  "bugs": "https://github.com/funte/google-closure-library-webpack-plugin/issues",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc --downlevelIteration --resolveJsonModule",
    "watch": "rimraf dist && tsc -w --downlevelIteration --resolveJsonModule",
    "test": "yarn run test:lint && yarn run test:all",
    "test:lint": "eslint . --ext .js",
    "test:all": "mocha --recursive 'test/**/*.test.js' --timeout 60000",
    "test:one": "mocha 'test/Sources.test.js' --timeout 60000 --trace-warnings",
    "updateExampleVersions": "node ./scripts/version.js"
  },
  "dependencies": {
    "@types/estree": "^0.0.46",
    "@types/webpack-sources": "^3.2.0",
    "fs-extra": "^9.1.0",
    "minimatch": "^3.0.4",
    "schema-utils": "^3.0.0",
    "slim-pig": "^1.3.5",
    "tapable": "^2.2.1",
    "typescript": "^4.3.5",
    "webpack-sources": "^3.2.3"
  },
  "peerDependencies": {
    "webpack": "^5.28.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.7",
    "@types/minimatch": "^3.0.5",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "commitizen": "^4.2.3",
    "enhanced-resolve": "^5.8.3",
    "eslint": "^7.22.0",
    "google-closure-library": "^20210202.0.0",
    "graceful-fs": "^4.2.8",
    "memfs": "^3.4.1",
    "memory-fs": "^0.5.0",
    "mocha": "^8.3.0",
    "rimraf": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
